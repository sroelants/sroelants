{
  "name": "sroelants.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build:sass": "node-sass -o src/_includes/assets/css/ src/_includes/assets/sass/*.sass --output-style compressed",
    "build:eleventy": "npx @11ty/eleventy",
    "serve": "browser-sync start --server 'dist' --files 'dist/*.html, dist/assets/css/*.css, dist/assets/js/*.js'",
    "watch:sass": "onchange 'src/assets/sass/*.sass' -- npm run build:sass",
    "watch:eleventy": "npx @11ty/eleventy --watch",
    "build:all": "npm run build:sass && npm run build:eleventy",
    "dev": "npm-run-all --parallel watch:* serve"
  },
  "keywords": [],
  "author": "Sam Roelants <roelants.sam@gmail.com> (http://sroelants.github.io/)",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "browser-sync": "^2.26.7",
    "clean-css": "^4.2.1",
    "eleventy-plugin-typeset": "^1.0.0",
    "markdown-it": "^9.0.1",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-katex": "^2.0.3",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "slugify": "^1.3.4",
    "typeset": "^0.2.2"
  }
}
